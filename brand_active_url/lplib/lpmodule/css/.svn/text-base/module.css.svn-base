}
.lp-select-items{
	min-width:148px;
	max-height:200px;
	border:1px solid #99bde2;
	background-color:#fff;
	position:absolute;
	z-index:999;
	visibility: hidden;
}
.lp-select-items-con{
	margin:3px 0;
}
.lp-select-item{
	list-style:none;
	height:24px;
	line-height:24px;
	margin:0;
	padding:0 8px;
	white-space: nowrap;
	cursor: pointer;
	display:block;
}
.lp-select-item-selected{
	background-color: #E7F2FE;	
}
.lp-select-multiple .lp-select-item-selected{
	background-color: #fff;	
}

.lp-select-item-hover,
.lp-select-multiple .lp-select-item-hover{
	background-color: #cfdff1;
}
.lp-select-item-checkbox{
	margin-right:3px;
	vertical-align: middle;
}
.lp-select-item s{
    color: #FF0000;
    text-decoration: none;
}
input.lp-select-search-input{
	background: url("http://img04.taobaocdn.com/tps/i4/T1OV97Xk0qXXXXXXXX-30-160.png") no-repeat scroll -8px -124px transparent;
	padding-left: 28px;
	width:94px;
	margin:1px;
}

