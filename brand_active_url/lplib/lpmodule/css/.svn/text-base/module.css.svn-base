/* tree begin */
.lp-inline-block {
    position: relative;
    display: -moz-inline-box; /* Ignored by FF3 and later. */
    display: inline-block;
}

* html .lp-inline-block {
    display: inline;
}

*:first-child+html .lp-inline-block {
    display: inline;
}

.lp-tree-root:focus {
  -moz-outline: none;
  outline: none;
}

.lp-tree-row {
  white-space: nowrap;
  font: icon;
  cursor: pointer;
  line-height: 16px;
  height: 16px;
}

.lp-tree-row span {
  overflow: hidden;
  text-overflow: ellipsis;
}

.lp-tree-children {
