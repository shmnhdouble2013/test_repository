/*Menu*/
.menu-container {
	height: 100%;
	background-color: #e2eaf4;
	border: 1px solid #99BCE8;
	text-align: left;
	color: #263E74;
	overflow-y: auto;
	overflow-x: hidden;
}
.menu-ul li {
	background: #e2eaf4;
	float: left;
	width: 100%;
}
.menu-li {
	padding-top: 1px;
	outline: none;
}
.menu-item {
	margin: 0px 1px 0 1px;
	background: #c1d5ec;
	height: 21px;
	padding-left:10px;
	color: #263e74;
	
	font-weight: bold;
	overflow: hidden;
	padding-top:4px;
}
.menu-sp {
	border-top: 1px solid #94b2d9;
	border-bottom: 1px solid #c6d4e6;
}
.menu-p-content {
	overflow: hidden;
	display: block;
	height: 20px;
	cursor:pointer;
}
.menu-ul-next {
	margin-top: 5px;
}
.menu-li a {
	display: block; 
	zoom: 1;
}
.menu-li a:link {
	text-decoration: none;
	color: #263e74;
}
.menu-li a:hover {
	text-decoration: none;
	color: #ff0000;
}
.menu-li a:active {
	text-decoration: none;
	color: #263e74;
}
.menu-li a:visited {
	text-decoration: none;
	color: #263e74;
}
.menu-item-next, .menu-item-active, .menu-plus, .menu-minus {
	background: url(http://img02.taobaocdn.com/tps/i2/T16zJ5XnxBXXXXXXXX-180-100.png) -9999px -9999px no-repeat;
}
.menu-plus {

	background-position: 0 0;
	width: 6px;
	height: 9px;
	display: inline-block;
	padding-left:8px;
	_padding-left:5px;
	float:left;
	margin-top:4px;
}
.menu-minus {
	
	background-position: -57px 0;
	width: 6px;
	height: 9px;
	display: inline-block;
	padding-left:8px;
	_padding-left:5px;
	float:left;
	margin-top:4px;
}
.menu-item-next {
	height: 26px;
	background-position: -154px 4px;
	text-indent: 2.5em;
	float: left;
	font-family: simsun;
	width: 100%;
	padding-top: 1px;
	background-position: -154px 3px \9;
	overflow: hidden;
}
.menu-item-active {
	height: 26px;
	text-indent: 2.5em;
	color: #fff;
	font-weight: bold;
	font-family: simsun;
	width: 100%;
	padding-top: 1px;
	overflow: hidden;
}
.menu-item-active .menu-p-lf, .menu-item-active .menu-p-content, .menu-item-active .menu-p-rt {
	background: url(http://img04.taobaocdn.com/tps/i4/T1bJCkXjNeXXXXXXXX-2000-93.png) -9999px -9999px no-repeat;
	height: 20px;
	line-height: 20px;
}
.menu-item-next .menu-p-lf, .menu-item-next .menu-p-rt {
	display: none;
}
.menu-item-active .menu-p-lf {
	width: 13px;
	background-position: 0 -2px;
	float: left;
	margin-left: 11px;
	display: inline;
}
.menu-item-active .menu-p-content {
	width: 126px;
	background-position: 0 -37px;
	float: left;
	display: block;
	text-align: left;
	overflow: hidden;
	text-indent: 0.5em;
	overflow: hidden;
}
.menu-item-active .menu-p-rt {
	width: 13px;
	background-position: -39px -2px;
	float: left;
	display: block;
}
.menu-ul-next-close {
	display: none;
}
/*Menu End Ëï¾¼¾ü  ºìÑà */
