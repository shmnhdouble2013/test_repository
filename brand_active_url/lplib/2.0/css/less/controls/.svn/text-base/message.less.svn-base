
.lp-contentbox{
	background-color: @dialogBackColor;
	padding-bottom:15px;
}

.lp-stdmod-header{
	height: 20px;
	line-height: 20px;
	color: #333;
	overflow: hidden;
	font-size: 14px;
	font-weight: bold;
	padding-left: 5px;
}

.lp-stdmod-header .x-icon{
	position:absolute;
	right: 4px;
  top: 4px;
	cursor: pointer;
}

.lp-stdmod-body{
	padding:0 35px;
	p{
		font-weight:normal;
	}
}
.lp-stdmod-body .x-icon{
	float:left;
}


.lp-message-content{
	margin:5px 0 0 40px;
	font-weight:bold;
	font-size:15px;
}
.lp-stdmod-footer{
	padding:20px 0 0 0;
	text-align:center;
}
.lp-stdmod-footer .mb-first{
	margin:0;
}
.lp-stdmod-footer .button{
	margin-right:10px;
}
.lp-message-btn{
	border:none;
	width:90px;
	height:25px;
	margin:0 0 0 30px;
	background-position:0px -55px;
	color:#fff;
	font-weight:bold;
	font-size:15px;
	cursor:pointer;
}